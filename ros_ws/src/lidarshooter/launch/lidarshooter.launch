<launch>
    <node pkg="lidarshooter" type="meshprojector" name="lidar_0000" output="screen">
        <param name="configfile" value="/workspaces/lidarshooter/ros_ws/hesai-pandar-XT-32.json"/>
    </node>
    <node pkg="lidarshooter" type="meshprojector" name="lidar_0001" output="screen">
        <param name="configfile" value="/workspaces/lidarshooter/ros_ws/hesai-pandar-XT-32.json"/>
    </node>
    <node pkg="lidarshooter" type="objtracker" name="objtracker" output="screen">
        <param name="meshfile" value="/workspaces/lidarshooter/ros_ws/ben.stl"/>
    </node>
    <node pkg="teleop_twist_keyboard" type="teleop_twist_keyboard.py" name="teleop_twist_keyboard" ns="/joystick" output="screen">
        <param name="speed" value="0.09"/>
        <param name="turn" value="0.15"/>
    </node>
</launch>
