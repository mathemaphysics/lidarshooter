{
    "version": "0.2.0",
    "configurations": [
        {
            "name": "(gdb) Debug LiDARShooterGUI",
            "type": "cppdbg",
            "request": "launch",
            "program": "/workspaces/lidarshooter/ros_ws/devel/lib/lidarshooter/LiDARShooterGUI",
            "args": [],
            "stopAtEntry": false,
            "cwd": "${fileDirname}",
            "environment": [
                {
                    "name": "ROS_MASTER_URI",
                    "value": "http://127.0.0.1:11311"
                },
                {
                    "name": "ROS_IP",
                    "value": "127.0.0.1"
                }
            ],
            "externalConsole": false,
            "MIMode": "gdb",
            "setupCommands": [
                {
                    "description": "Enable pretty-printing for gdb",
                    "text": "-enable-pretty-printing",
                    "ignoreFailures": true
                },
                {
                    "description": "Set Disassembly Flavor to Intel",
                    "text": "-gdb-set disassembly-flavor intel",
                    "ignoreFailures": true
                }
            ]
        },
