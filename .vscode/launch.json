{
    "version": "0.2.0",
    "configurations": [
        {
            "name": "(gdb) Debug LiDARShooterGUI",
            "type": "cppdbg",
            "request": "launch",
            "program": "/workspaces/lidarshooter/ros_ws/devel/lib/lidarshooter/LiDARShooterGUI",
            "args": [],
            "stopAtEntry": false,
            "cwd": "${fileDirname}",
            "environment": [],
            "externalConsole": false,
            "MIMode": "gdb",
            "setupCommands": [
                {
                    "description": "Enable pretty-printing for gdb",
                    "text": "-enable-pretty-printing",
                    "ignoreFailures": true
                },
                {
                    "description": "Set Disassembly Flavor to Intel",
                    "text": "-gdb-set disassembly-flavor intel",
                    "ignoreFailures": true
                }
            ]
        },
        {
            "name": "(gdb) Debug OptixTracer Tests: TraceSceneCloud",
            "type": "cppdbg",
            "request": "launch",
            "program": "/workspaces/lidarshooter/ros_ws/devel/lib/lidarshooter/OptixTracer_test",
            "args": ["--gtest_filter=OptixTracerTest.TraceSceneCloud"],
            "stopAtEntry": false,
            "cwd": "${fileDirname}",
            "environment": [],
            "externalConsole": false,
            "MIMode": "gdb",
            "setupCommands": [
                {
                    "description": "Enable pretty-printing for gdb",
                    "text": "-enable-pretty-printing",
                    "ignoreFailures": true
                },
                {
                    "description": "Set Disassembly Flavor to Intel",
                    "text": "-gdb-set disassembly-flavor intel",
                    "ignoreFailures": true
                }
            ]
        }
    ]
}